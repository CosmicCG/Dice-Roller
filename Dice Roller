def DiceRoller():
    import random
    random = random.randint(0,6)
    if (random == 1):
        print("1")
    if (random == 2):
        print("2")
    if (random == 3):
        print("3")
    if (random == 4):
        print("4")
    if (random == 5):
        print("5")
    if (random == 6):
        print("6")


DiceRoller()
